{
  "name": "jack-barry-onl",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "npm run compile-index && vite build",
    "build:e2e": "cross-env IS_TEST=true npm run build",
    "check": "svelte-kit sync && npm run svelte-check",
    "check:watch": "svelte-kit sync && npm run svelte-check -- --watch",
    "compile-index": "tsx scripts/compile_content_index.ts",
    "compile-index:watch": "chokidar \"content/**/*.md\" -c \"npm run compile-index\"",
    "dev": "concurrently --names=vite,compile-index --prefix-colors=green,blue \"vite dev\" \"npm run compile-index:watch\"",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "playwright:ui": "playwright test --ui",
    "preview": "vite preview",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "playwright test",
    "test:unit": "vitest run --passWithNoTests"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@eslint/js": "9.17.0",
    "@playwright/test": "1.49.1",
    "@poppanator/sveltekit-svg": "5.0.0",
    "@popperjs/core": "2.11.8",
    "@resvg/resvg-js": "2.6.2",
    "@sveltejs/adapter-vercel": "5.5.2",
    "@sveltejs/kit": "2.15.1",
    "@sveltejs/vite-plugin-svelte": "4.0.4",
    "@types/bootstrap": "5.2.10",
    "@types/mdast": "4.0.4",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "bootstrap": "5.3.3",
    "chokidar-cli": "3.0.0",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.46.1",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.3.2",
    "sass": "1.77.6",
    "satori": "0.12.1",
    "satori-html": "0.3.2",
    "svelte": "5.16.4",
    "svelte-check": "4.1.1",
    "tslib": "2.8.1",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.19.1",
    "vite": "5.4.11",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "@types/hast": "3.0.4",
    "@types/node": "22.10.5",
    "animate.css": "4.1.1",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "classnames": "2.5.1",
    "dayjs": "1.11.13",
    "gray-matter": "4.0.3",
    "highlight.js": "11.11.1",
    "marked": "15.0.6",
    "marked-highlight": "2.2.1",
    "rehype-raw": "7.0.0",
    "rehype-unwrap-images": "1.0.0",
    "remark": "15.0.1",
    "remark-frontmatter": "5.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.1",
    "strip-markdown": "6.0.0",
    "unified": "11.0.5",
    "unist-util-visit": "5.0.0"
  }
}
